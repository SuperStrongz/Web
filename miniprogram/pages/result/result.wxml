<view>
  <view class="toptitle">WeChat</view>
  <view class="subtitle">最适合你的职业推荐</view>
  <view class="list">
    <view wx:for="{{profession}}" wx:key="profession" class="profession {{currentTabsIndex == index ? 'clicked' : ''}}" bindtap="changeColor" data-index="{{index}}">
      {{profession[index].name}}
    </view>
    <view wx:for="{{title}}" class="title" wx:key="title">
      {{title[index]}}  {{filtrateResult[index]}}
    </view>
  </view>
  <view class="studyPeople">2753人在学</view>
</view>